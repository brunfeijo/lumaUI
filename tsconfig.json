{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress"
        ],
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "allowJs": true
    },
    "include": [
        "cypress/**/*.ts"
, "cypress/support/e2e.ts"    ]
}